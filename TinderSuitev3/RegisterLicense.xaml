<Window x:Class="TinderSuitev3.RegisterLicense"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TinderSuitev3"
        Style="{StaticResource CustomWindowStyle}"
        mc:Ignorable="d"
        SizeToContent="Height"
        Title="Register License" Height="117" Width="248">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock FontWeight="SemiBold" Margin="0, 0, 0, 5">License Key:</TextBlock>
            <TextBox Name="LicenseKey" Padding="3"></TextBox>
            <Button Name="Register" Click="Register_OnClick" Content="Register License" Padding="3" Margin="0, 10, 0, 0"></Button>
        </StackPanel>
    </Grid>
</Window>
